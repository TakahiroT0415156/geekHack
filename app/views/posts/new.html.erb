
<div class= "formmmm" >
<%= form_for @post do |f| %>
  <%= f.label :name, "投稿者" %><br />
  <%= f.text_field :name %><br />
  <%= f.label :title, "タイトル" %><br />
  <%= f.text_field :title %><br />
  <%= f.label :word, "言語" %><br />
  <%= f.text_field :word %><br />
  <%= f.label :body, "内容" %><br />
  <%= f.text_area :body %><br />
  <%= f.submit "投稿する" %><br />
<% end %>
</div>